<h1><%=t('.new') %></h1>
<%= form_for(@group) do |f| %>

<%= f.hidden_field :user_id %>
<div class="field">
      <%= f.label :name %></br>
	  <%= f.text_field :name %>
</div>

<div class="field">
      <%= f.label :user_tokens, 'Members' %></br>
	  <%= f.text_field :user_tokens, "data-pre" => @group.users.to_json %>
</div>
</br>
<div class="actions">
      <%= f.button :submit %>
</div>

<% end %>
