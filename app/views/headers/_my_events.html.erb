<%= javascript_tag do %>
  window.searcheventURL = '<%= j search_path %>';
  window.neweventURL = '<%= j new_event_path %>';
<% end %>
<div class='row-fluid'>
<div class="i_myevents pull-left">
<% if user_signed_in? %>My Events</div>
<%unless current_user.admin?%><div class="pull-right visible-desktop" id='host_event'><%=t('host')%></div>
<%end%>
<div class=" pull-right visible-desktop" id='search_event'><%=t('search')%> EVENTS</div>
<%else%>
<%=t('search_results')%>
</div>
<%end%>
</div>
